{
  "config": {
    "enable_plots": true,
    "save_plots": true,
    "show_plots": false,
    "enable_tf_determinism": false,
    "save_best_model": false,
    "convert_to_coreml": false,
    "unfreeze_layers": false,
    "random_seed": 42,
    "enable_mixed_precision": true,
    "use_early_stopping": true
  },
  "hyperparameters": {
    "unfreeze_last_n_layers": 10,
    "batch_size": 64,
    "img_size": [224, 224],
    "channels": 3,
    "learning_rate": 1e-4,
    "num_classes": 3,
    "epochs": 250,
    "early_stopping_patience": 25,
    "dropout_rate": 0.25,
    "gaussian_noise_stddev": 0.25,
    "num_attention_heads": 8,
    "attention_key_dim": 3,
    "train_split": 0.8,
    "valid_test_split": 0.5,
    "optimizer": "Adam",
    "loss_function": "sparse_categorical_crossentropy",
    "metrics": ["accuracy"],
    "other_metrics": {
      "unknown": ["MeanAbsoluteError"],
      "pass": ["accuracy", "SparseCategoricalAccuracy"],
      "fail": ["Precision", "MeanSquaredError", "Recall", "AUC"]
    }
  },
  "augmentation": {
    "rotation_range": 180,
    "width_shift_range": 0.1,
    "height_shift_range": 0.1,
    "shear_range": 0.1,
    "zoom_range": 0.1,
    "horizontal_flip": true,
    "vertical_flip": true,
    "fill_mode": "nearest",
    "augmenations_to_test": {
      "brightness_range": [0.7, 1.3],
      "contrast_range": [0.7, 1.3],
      "blur_range": [0.1, 0.3],
      "noise_range": [0.1, 0.3]
    },
    "disallowed_augmentations": {
      "hue_shift_range": null,
      "staturation_shift_range": null,
      "value_shift_range": null,
      "crop": null
    }
  }
}
