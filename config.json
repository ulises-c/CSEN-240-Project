    "base_model": "Xception",
    "base_model_weights": "imagenet",
    "base_model_include_top": false,
    "other_models": {
      "tested": [
        "Xception",
        "EfficientNetV2S",
        "EfficientNetV2M",
        "EfficientNetV2L"
      ],
      "untested": [
        "ResNet50V2",
        "InceptionV3",
        "MobileNetV2",
        "DenseNet121",
        "VGG16"
      ]
    },
    "other_weights": null,
    "progressive_unfreeze_schedule": {
      "0": 0,
      "10": 0.05,
      "20": 0.15,
      "30": 0.3,
      "40": 0.5,
      "50": 0.75
    }
  },
  "augmentation": {
    "rotation_range": 180,
    "width_shift_range": 0.1,
    "height_shift_range": 0.1,
    "shear_range": 0.1,
    "zoom_range": 0.1,
    "horizontal_flip": true,
    "vertical_flip": true,
    "fill_mode": "nearest",
    "brightness_range": [0.7, 1.3],
    "augmenations_to_test": {
      "contrast_range": [0.7, 1.3],
      "blur_range": [0.1, 0.3],
      "noise_range": [0.1, 0.3]
